/* pages/futian/futian.wxss */
page{
  background: #f2f2f2;
}
.top{
  width: 100%;
}
.menu{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  font-size: 9pt;
  margin-bottom: 3pt;
}
.menu view{
  width: 23%;
  padding: 5pt 0;
  background: white;
  text-align: center;
}
.list{
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  font-size: 10pt;
  background: white;
  margin-bottom: 1px;
}
.list view{
  width: 23%;
  padding: 5pt 0;
  background: white;
  text-align: center;
}
.icon{
  width: 20pt;
  height: 20pt;
  border-radius: 50%;
}