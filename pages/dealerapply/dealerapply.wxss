.mid {
  position: absolute;
  top: 20%;
  width: 100%;
  text-align: center;
}

.mid image {
  width: 60%;
}

.jump {
  width: 50pt;
}

.account {
  width: 86%;
  padding: 0 7%;
  background: white;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  position: absolute;
  bottom: 10%;
  font-size: 12pt;
  color: #333;
}

.kuang {
  position: absolute;
  top: 20%;
  width: 100%;
  text-align: center;
}

.kb {
  width: 70%;
}

.kcontent {
  position: absolute;
  bottom: 8%;
  width: 60%;
  left: 20%;
  height: 55%;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
}

.km {
  color: #333;
}

.kt {
  width: 100%;
}

.av {
  width: 40%;
  left: 30%;
  height: 30pt;
  line-height: 30pt;
  text-align: center;

  position: absolute;
  bottom: 6%;
  font-size: 12pt;
  color: white;
  background: linear-gradient(to right, #20c8f5, #0fa4f6);
  border-radius: 3pt;
}
.share{
  width: 18%;
  position: absolute;
  right: 0;
  top: 5%;
}
.sbutton{
  width: 18%;
  position: absolute;
  right: 0;
  top: 5%;
  height: 30pt;
  background: transparent;
}
.dq{

  padding: 10pt 5pt;
  position: absolute;
  top: 0;
  left: 0;
  font-size: 12pt;
}
.sj{
  position: absolute;
  top:26%;
  left: 36%;
  width: 28%;
}